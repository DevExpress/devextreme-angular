{
    "compilerOptions": {
        "baseUrl": "./",
        "skipLibCheck": true,
        "target": "ES5",
        "module": "commonjs",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "inlineSources": true,
        "removeComments": false,
        "declaration": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true
    }
}
